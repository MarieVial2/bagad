{% extends 'baseAdmin.html.twig' %}

{% block title %}Evenement{% endblock %}

{% block body %}
<div class="container">
    <h4>Evenement</h4>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ evenement.id }}</td>
            </tr>
            <tr>
                <th>NomEvenement</th>
                <td>{{ evenement.nomEvenement }}</td>
            </tr>
            <tr>
                <th>DateEvenement</th>
                <td>{{ evenement.dateEvenement ? evenement.dateEvenement|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>HeureDebutEvenement</th>
                <td>{{ evenement.heureDebutEvenement }}</td>
            </tr>
            <tr>
                <th>HeureFinEvenement</th>
                <td>{{ evenement.heureFinEvenement }}</td>
            </tr>
            <tr>
                <th>LieuEvenement</th>
                <td>{{ evenement.lieuEvenement }}</td>
            </tr>
            <tr>
                <th>PhotoEvenement</th>
                <td>{{ evenement.photoEvenement }}</td>
            </tr>
            <tr>
                <th>DescriptionEvenement</th>
                <td>{{ evenement.descriptionEvenement }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_evenement_index') }}">back to list</a>

    <a href="{{ path('app_evenement_edit', {'id': evenement.id}) }}">edit</a>

    {{ include('evenement/_delete_form.html.twig') }}
</div>
{% endblock %}
